pipeline {
    agent any

    tools {
        // Install the Maven version configured as "M3" and add it to the path.
        maven "M3"
    }

    stages {
        stage('Compile') {
            steps {
                echo "test mvn complie"
            }

            }
stage('Test') {
            steps {
               echo "test mvn complie"
            }

            }
stage('package') {
            steps {
             echo "test mvn package" 
            }

            }
stage('Install') {
            steps {
               echo "test mvn complie"
            }

            }
        }
}
